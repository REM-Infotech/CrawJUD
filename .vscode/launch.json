  "configurations": [
    {
      "name": "Python Debugger: Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "quart",
      "env": { "FLASK_APP": "app.py", "FLASK_DEBUG": "1" },
      "args": ["start"]
    },
    {
      "name": "Python Debugger: Celery",
      "type": "debugpy",
      "request": "launch",
      "module": "celery",
      "env": { "CELERY_APP": "celery_app" },
      "args": ["worker", "-P", "threads", "-E"]
    }
  ],

  "version": "0.2.0"
}
