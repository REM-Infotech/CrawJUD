{
  "name": "crawjud-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --host",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vue-styled-components/core": "^1.13.3",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "^0.30.5",
    "chart.js": "^4.5.0",
    "datatables.net-bs5": "^2.3.4",
    "datatables.net-dt": "^2.3.4",
    "datatables.net-vue3": "^3.0.4",
    "pinia": "^3.0.3",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.22",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.6",
    "@iconify-json/material-symbols": "^1.2.40",
    "@iconify-json/material-symbols-light": "^1.2.40",
    "@iconify-json/ri": "^1.2.5",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.7",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/compiler-sfc": "^3.5.22",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.5.4",
    "eslint": "^9.36.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-oxlint": "~1.1.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.6.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.1.0",
    "prettier": "3.5.3",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.8.3",
    "unplugin-icons": "^22.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}